use Students // use employees database
db // show current database



// collections
db.createCollection()// Create collection
db.Billal.drop() // delete collection
// db.{collection name }.insertOne({"type hare your data"})
db.Billal.insertOne({"type hare your data"})
db.Billal.insertMany([{"type hare your data"},{type here another data}])
db.Billal.find().pretty()

// db.{collection name}.find(query,projection)
db.Billal.find({name: "joy"}).pretty()
db.Billal.find({name: "joy"}, {name:1}).pretty()
db.Billal.find({name: "joy"}, {name:0}).pretty()
db.Billal.find({name: "joy"}, {_id: 0 ,name:1}).pretty()
db.Billal.find().pretty().limit(2)



